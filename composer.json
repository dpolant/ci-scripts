{
    "name": "mediacurrent/ci-scripts",
    "description": "Scripts helpful for testing a Drupal site on a Continuous-Integration server, or locally.",
    "bin": ["bin/hobson"],
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "joestewart/robo-drupalvm": "^0.1.3",
        "joestewart/robo-vagrant": "^0.1.1",
        "boedah/robo-drush": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.4"
    },
    "autoload": {
        "psr-4": {
            "Mediacurrent\\CiScripts\\": "src"
        }
    }
}
